    font-size: 24px;
    font-weight: bold;
    margin: 20px 0 30px 0;
}
.dm-container hr{
    width: 100%;
    color:#cbd0d3;
}
.d-bottom{
    height: 40px;
    line-height: 40px;
}
.d-bottom a{
    font-size: 12px;
    color:#999999;
}

