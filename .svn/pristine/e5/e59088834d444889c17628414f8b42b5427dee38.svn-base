/*�˻���Ϣ*/
.baseInfo,.stuCertification,.changePwd,#myShoppingCart ul,.integralCorrelation,.commodityRule,.myShoppingCart,.myRelease{
    background: #fff;
    overflow: hidden;
    margin-top: 20px;
    padding: 20px 0;
}
.userLeft,.userRight{
    width: 467.5px;
    float: left;
}
.userLeft p{
    line-height: 30px;
    margin:20px 0;
}
.userLeft p label{
    display: inline-block;
    width: 130px;
    text-align: right;
    height: 30px;
    margin-right: 20px;
}
.userLeft p input{
    padding-left: 5px;
    height: 30px;
    width: 300px;
}
.userLeft>a{
    display: block;
    text-align: center;
    width: 120px;
    height: 40px;
    line-height: 40px;
    background: #3c83c5;
    color: #fff;
    font-size: 18px;
    border: 0;
    border-radius: 10px;
    margin: 0 auto;
}
.userRight .headImg{
    border:1px solid #8bc3f7;
    width: 140px;
    height: 140px;
    margin: 30px auto 10px;
}
.userRight .headImg img{
    margin: 10px;
}
.userRight p{
    text-align: center;
}
/*ѧ����֤���޸�����*/
.stuCertification>p{
    width: 768px;
    margin: 20px auto;
    padding: 10px 30px;
    line-height: 25px;
    border: 1px solid #3c83c5;
}
.stuCertification form p,.changePwd form p{
    margin:20px auto;
}
.stuCertification form label,.changePwd form label{
    display: inline-block;
    width: 150px;
    text-align: right;
    line-height: 30px;
    margin-right: 20px;
}
.stuCertification form input,.changePwd form input{
    height: 30px;
    width: 300px;
    padding-left: 5px;
}
.stuCertification form p button{
    width: 120px;
    height: 23px;
    color: #fff;
    background: #36a5dd;
    border: 0;
}
.stuCertification form p span{
    color: #ff0000;
    margin-left: 20px;
}
.stuCertification form>span{
    color: #8e8e8e;
    font-size: 12px;
    margin-left: 170px;
    position: relative;
    top:-20px;
}
.stuCertification form>button,.changePwd form>button{
    display: block;
    width: 120px;
    height: 40px;
    color: #fff;
    background: #3c83c5;
    border: 0;
    border-radius: 10px;
    margin-left: 175px;
}
/*************************�ҵĹ��ﳵ,�ҵĶ���,�ҵ��ղ�*/
.myShoppingCart table{
    width: 895px;
    border: 1px solid #ddd;
    margin: 10px 20px;
    border-collapse: separate;
    border-spacing: 0;
}
.myShoppingCart table th{
    height: 40px;
    line-height: 40px;
    background: #f0f1f2;
    border: 0.5px solid #ddd;
}
.myShoppingCart table th:first-child,.myShoppingCart table td:first-child{
    width: 60%;
}
.myShoppingCart table th:last-child,.myShoppingCart table td:last-child{
    width: 20%;
}
.myShoppingCart table td{
    border: 1px solid #ddd;
    height: 100px;
    overflow: hidden;
    box-sizing: border-box;
}
.myShoppingCart table .pic{
    float: left;
    width: 70px;
    height: 70px;
    margin: 15px;
    box-sizing: border-box;
    border:1px solid #FF0700;
}
.myShoppingCart table .pic img{
    margin: 10px;
    width: 50px;
    height: 50px;
}
.myShoppingCart table div{
    padding:10px;
    float: left;
}
.myShoppingCart table div p{
    line-height: 40px;
}
.myShoppingCart table div p.owner{
    color: #808080;
}
.myShoppingCart table .price{
    color: #ff0000;
    font-size: 16px;
    text-align: center;
}
.myShoppingCart table td:last-child button{
    display: block;
    background: #3c83c5;
    width: 120px;
    height: 30px;
    color: #fff;
    border:0;
    border-radius: 8px;
    margin: 12px auto;

}
.myShoppingCart table td:last-child a{
    display: block;
    color: #ff0000;
    text-align: center;
}
.myShoppingCart>p{
    text-align: center;
}
.myShoppingCart>p>a{
    display: inline-block;
    width: 65px;
    height: 30px;
    border: 1px solid #999797;
    border-radius: 5px;
    color: #4d4d4d;
    text-align: center;
    line-height: 30px;
    margin: 10px;
}
.myShoppingCart>p>a.curr{
    background: #3c83c5;
    color: #fff;
}
/******************************�ҵĶ���*/
#myOrder h2 li{
    display: inline-block;
}
#myOrder h2 li a{
    display: inline-block;
    margin-right: 10px;
    width: 140px;
    height: 35px;
    line-height:35px;
    text-align: center;
    color: #323232;
    font-size: 18px;
    border-radius: 10px 10px 0 0;
    background: #fefefe;
    position: relative;
    top:3px;
}
#myOrder h2 li.curr a{
    background: #3c83c5;
    color: #fff;
}
#myOrder .myShoppingCart{
    display: none;
}
#myOrder>.curr{
    display: block;
}
