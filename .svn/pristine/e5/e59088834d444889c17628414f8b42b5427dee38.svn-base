#myRelease h2 a{
    float: right;
    width: 120px;
    height: 30px;
    line-height: 30px;
    font-weight: normal;
    text-align: center;
    background: #3c83c5;
    color: #fff;
    border-radius: 10px;
    margin-top: 10px;
    margin-right: 20px;
}
.myRelease ul{
    text-align: center;
    margin-left:20px;
}
.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover {
    z-index: 3;
    color: #fff;
    cursor: default;
    background-color: #337ab7;
    border-color: #337ab7;
}
.pagination>li {
    display: inline;
}
.pagination>li>a, .pagination>li>span {
    position: relative;
    float: left;
    padding: 6px 12px;
    margin-left: -1px;
    line-height: 1.42857143;
    color: #337ab7;
    text-decoration: none;
    background-color: #fff;
    border: 1px solid #ddd;
}
.pagination {
    display: inline-block;
    padding-left: 0;
    margin: 20px 0;
    border-radius: 4px;
}
.myRelease ul li {
	display: inline-block;
    text-align: left;
    float:left;
    width: 224px;
    overflow: hidden;
    box-sizing: border-box;
    padding: 10px;
    border: 2px solid #999999;
    margin: 0 18px 20px 0;
}
#myRelease ul li img{
 
    width: 185px;
    height: 185px;
    margin: 0 auto;
    border: 1px solid #FF0700;
}
#myRelease ul li p{
    line-height: 30px;
    margin-left: 10px;
}
#myRelease ul li p span{
    color: #FF0700;
}
#myRelease ul li p.title{
    font-size: 18px;
}
#myRelease ul li div{
    background: #fff;
    height: 60px;
    line-height: 60px;
    text-align: center;
}
#myRelease ul li div button{
    width: 29%;
    height: 30px;
    float: left;
    background: #fff;
    border: 1px solid #ccc;
    margin: 15px 5px;
}
.myRelease>p{
    text-align: center;
}
.myRelease>p>a{
    display: inline-block;
    width: 65px;
    height: 30px;
    border: 1px solid #999797;
    border-radius: 5px;
    color: #4d4d4d;
    text-align: center;
    line-height: 30px;
    margin: 10px;
}
.myRelease>p>a.curr{
    background: #3c83c5;
    color: #fff;
}

