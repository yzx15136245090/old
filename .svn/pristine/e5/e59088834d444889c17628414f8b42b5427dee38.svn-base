section{
    width: 100%;
    background: #f0f1f2;
    padding: 20px 0;

}
#main{
    overflow: hidden;
}
.myApp{
    width: 225px;
    box-sizing: border-box;
    border: 1px solid #ccc;
    background: #fff;
    float: left;
}
.myApp ul li{
    height: 40px;
    line-height: 40px;
    color: #333;
    padding-left: 40px;
}
.myApp ul li a:hover{
    color: #36a5dd;
}
.myApp ul li.title{
    height: 50px;
    line-height: 50px;
    background: #e8e8e8;
    padding-left: 20px;
    font-size: 16px;
    color: #000;
}
.myApp ul li.curr{
    background: #36a5dd;
}
.myApp ul li.curr a{
    color: #fff;
}
#accountInfo,#stuCertification,#changePwd,#IntegralRule,#commodityRule,#myShoppingCart,#myRelease,#myOrder,#myCollection,#shelfGoods{
    width: 935px;
    float: right;
    box-sizing: border-box;
    display: none;
}
#userCenter{
    width: 935px;
    float: right;
    box-sizing: border-box;
}
#userCenter .baseInfo{
    height: 200px;
    border: 2px solid #3c83c5;
    background: #ddeef6;
    box-sizing: border-box;
    overflow: hidden;
}
#userCenter .baseInfo .changeHeadImg{
    width: 140px;
    margin:0 20px;
    float: left;
}
#userCenter .baseInfo .changeHeadImg .headImg{
    background: #fff;
    padding: 10px;
}
#userCenter .baseInfo .changeHeadImg p{
    text-align: center;
    line-height: 40px;
}
#userCenter .baseInfo .userInfo{
    float: left;
    width: 700px;
    margin-left: 20px;
}
#userCenter .baseInfo .userInfo p{
    line-height: 32px;
    border-bottom:  1px dashed #ccc;
    color: #808080;
}
#userCenter .baseInfo .userInfo p:last-child{
    border-bottom: 0;
}
#userCenter .baseInfo .userInfo p span{
    color: #000;
}
#userCenter .baseInfo .userInfo p i{
    color: #FF0700;
}
#userCenter .myPro{
    background: #fff;
    height: 85px;
    margin-top: 40px;
}
#userCenter .myPro table{
    margin: 0 100px;
    padding-top: 10px;
}
#userCenter .myPro td{
    width: 150px;
    text-align: center;
    line-height: 30px;
}
#userCenter .myPro td i{
    color: #36a5dd;
}
#userCenter>span{
    color: #ff0000;
    float: right;
    line-height: 40px;
}

#main > .curr{
    display: block;
}
section h2{
    font-size: 20px;
    font-weight: normal;
    color: #3c83c5;
    height: 48px;
    line-height: 48px;
    border-bottom: 2px solid #36a5dd;
    padding-left: 20px;
    background: #FFF;
    margin-bottom: 20px;
}
