*{
    box-sizing: border-box;
}
/*首页页面样式*/
#container_banner,#hot_recommended,#product_show{
    margin:0 auto;
    position: relative;
    width:1200px;
}
#nav{
    margin:0 auto;
    width:1200px;
    height: 40px;
}
img{
    margin:0;
    padding:0;
}
#head_main,#head2_main{
    width:100%;
    height: 173px;
}
#head2_main{
    border-bottom:0;
}
#headMain_content{
    padding:35px 0;
}
/*导航*/
#main_nav{
    width: 100%;
    height: 40px;
    line-height: 40px;
    background:#3C83C5;
}
#nav li{
    float:left;
    height: 40px;
    width:120px;
    text-align:center;
}
#nav li:hover{
    background: #3572AB;
}
#nav li a{
    color:#fff;
    font-size: 16px;
}
.pructNav{
    position:relative;
    text-align: center;
    height: 54px;
    top:-6px;
    float:left;
    z-index: 2000;
    line-height: 54px;
    width:224px;
    background-color:#498FD1;
    background-image:url(../../index/images/down.png);
    background-repeat: no-repeat;
    background-position:190px center;
}
.pructNav a{
    color:#fff;
    font-size: 18px;
}
#nav .pructNav ul.pull-menu{
    top:54px;
    position: absolute;
    width: 224px;
    height:500px;
    background:#fff;
    z-index: 3000;
    display: none;
}
#nav .pructNav ul.pull-menu li{
    width:224px;
    cursor:pointer;
    text-align: left;
    height: 50px;
    line-height: 50px;
}
#nav .pructNav ul.pull-menu li a{
    color: #3c83c5;
    width:224px;
    padding-left:75px;
    display: block;
}
#nav .pructNav ul.pull-menu li.subnav-click{
    background:#3C83C5 ;
}
#nav .pructNav ul.pull-menu li.subnav-click a{
    color:#fff;
}
#nav .pructNav ul.pull-menu li:first-child a{
    background: url(../../index/images/demo_list2.png) no-repeat 43px 16px;
}
#nav .pructNav ul.pull-menu li:nth-child(2) a{
    background: url(../../index/images/demo_list2.png) no-repeat 43px -33px;
}
#nav .pructNav ul.pull-menu li:nth-child(3) a{
    background: url(../../index/images/demo_list2.png) no-repeat 43px -78px;
}
#nav .pructNav ul.pull-menu li:nth-child(4) a{
    background: url(../../index/images/demo_list2.png) no-repeat 43px -139px;
}
#nav .pructNav ul.pull-menu li:nth-child(5) a{
    background: url(../../index/images/demo_list2.png) no-repeat 40px -190px;
}
#nav .pructNav ul.pull-menu li:nth-child(6) a{
    background: url(../../index/images/demo_list2.png) no-repeat 43px -248px;
}
#nav .pructNav ul.pull-menu li:nth-child(7) a{
    background: url(../../index/images/demo_list2.png) no-repeat -179px 14px;
}
#nav .pructNav ul.pull-menu li:nth-child(8) a{
    background: url(../../index/images/demo_list2.png) no-repeat 43px -319px;
}
#nav .pructNav ul.pull-menu li:nth-child(9) a{
    background: url(../../index/images/demo_list2.png) no-repeat -179px -31px;
}
#nav .pructNav ul.pull-menu li:hover{
    background: #3c83c5;
}
#nav .pructNav ul.pull-menu li:hover a{
    color:#fff;
}
.u-deepBlue{
    background: #3572AB;
}
#head2_nav{
    height:35px;
    line-height: 35px;
}
#head2_nav{
    width: 100%;
    background: #3ca3dd;
    margin-bottom:20px;
}
#head2_nav #nav li a{
    color:#fff;
}

.cotainer{
    width:100%;
}


